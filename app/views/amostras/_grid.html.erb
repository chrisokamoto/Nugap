<div class="row">
  <div class="col-lg-12">
    <div class="widget">
      <div class="widget-header">
        <div class="title">
          
        </div>
      </div>

      <div class="widget-body">
        <%= grid(@parametro_resultado_grid, html:{class: 'table table-condensed table-striped table-hover table-bordered'}, show_filters: :when_filtered) do |g|

          g.column name: "Tipo Análise", attribute: 'tipo'          
          g.column name: "Parâmetro", attribute: 'parametro'
          g.column name: t(:resultado), attribute: 'resultado'
          g.column name: "Conclusão", attribute: 'conclusao'          

          g.column name: "Editar" do |parametro_resultado|
              link_to content_tag(:p, content_tag(:i, nil, class: "fa fa-pencil-square-o fa-2x", id: "editar_parametro_resultado"), class: "text-warning icon_edit expose"), '#', onclick:"edit_parametro_resultado(#{parametro_resultado.id});", id: "editar-#{parametro_resultado.id}"
            end            

            g.column name: "Apagar" do |parametro_resultado|
              link_to content_tag(:p, content_tag(:i, nil, class: "fa fa-trash-o fa-2x", id: "deletar_parametro_resultado"), class: "delete-row text-muted icon_delete expose"), '#', onclick:"delete_parametro_resultado(#{parametro_resultado.id});", id: "deletar-#{parametro_resultado.id}"
            end

        end -%>
      </div>
    </div>
  </div>
</div>