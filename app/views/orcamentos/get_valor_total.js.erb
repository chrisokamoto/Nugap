<% if(request.original_url.include? "orcamentos") %>
	document.getElementById("addButtonServico").disabled = false; 
	$('#servico_orcamento_valor_total').val("<%= @preco_total %>");		
	
<% end %>

function limpa(){
	<% if @tem_quantidade == 0 %>
		$('#servico_orcamento_qtd_amostra').val("");
	<%end%>
}

function zera(){	
	<% if @tem_quantidade == 0 %>
		$('#servico_orcamento_qtd_amostra').val("0");
	<%end%>
}

